{
  "name": "@tinykode/gen",
  "version": "0.0.7",
  "private": false,
  "description": "Generate bash commands from natural language using existing AI tools (GitHub Copilot CLI, Gemini CLI)",
  "type": "module",
  "main": "./src/index.js",
  "bin": {
    "gen": "src/index.js"
  },
  "scripts": {
    "pack-and-publish": "npm version patch && npm run pack && npm publish",
    "postinstall": "node scripts/install.js",
    "configure": "node scripts/install.js",
    "test": "LOG_LEVEL=0 node --test"
  },
  "keywords": [
    "bash",
    "shell",
    "ai",
    "copilot",
    "gemini",
    "cli",
    "command-generation",
    "natural-language"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "preferGlobal": true,
  "os": [
    "darwin",
    "linux"
  ],
  "publishConfig": {
    "access": "public"
  }
}